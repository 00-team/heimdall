[Unit]
Description=heimdall dog
After=syslog.target

[Service]
Type=simple
Restart=always
WorkingDirectory=/x/heimdall/dog/
EnvironmentFile=/x/heimdall/dog/.env
ExecStart=/x/heimdall/dog/target/release/dog
KillSignal=SIGQUIT
StandardError=file:/x/heimdall/dog/log
NotifyAccess=all

[Install]
WantedBy=multi-user.target
